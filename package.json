{
  "name": "@opengsn/paymasters",
  "version": "2.0.0-beta.1.3",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@opengsn/gsn": "2.0.0-beta.1.3",
    "@openzeppelin/contracts": "3.1.0",
    "@openzeppelin/upgrades": "2.8.0",
    "@types/source-map-support": "^0.5.1",
    "run-with-testrpc": "^0.3.1",
    "web3-core-helpers": "1.2.9",
    "web3-eth-abi": "1.2.9",
    "web3-eth-contract": "1.2.9",
    "web3-utils": "1.2.9"
  },
  "devDependencies": {
    "@openeth/truffle-typings": "0.0.6",
    "@openzeppelin/test-helpers": "^0.5.5",
    "@truffle/hdwallet-provider": "1.0.35",
    "@types/chai": "4.2.11",
    "@types/web3": "1.2.2",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-config-standard-with-typescript": "18.0.2",
    "eslint-plugin-import": "2.21.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "ethereumjs-tx": "2.1.2",
    "solhint": "3.0.0",
    "solidity-string-utils": "^0.0.4",
    "source-map-support": "0.5.16",
    "truffle": "5.1.28",
    "ts-node": "8.10.2",
    "typechain": "^2.0.0",
    "typechain-target-trufflehotfix": "0.0.4-alpha",
    "typescript": "3.9.2",
    "web3": "1.2.7"
  },
  "scripts": {
    "generate": "rm -rf ./build/contracts && npx truffle compile && typechain --target trufflehotfix './build/contracts/**/*.json'",
    "ci": "yarn prepack && yarn test",
    "test": "run-with-testrpc --hardfork 'istanbul' --gasLimit 100000000 'truffle test'",
    "ganache-with-gsn": "run-with-testrpc --gasLimit 100000000 'gsn start'",
    "tsc": "tsc --noEmit",
    "prepack": "./scripts/prepack",
    "truffle-compile": "rm -rf build/ && truffle compile",
    "lint": "yarn run lint:js && yarn run lint:sol",
    "lint:fix": "yarn run lint:js:fix",
    "lint:js": "eslint -f unix .",
    "lint:sol": "solhint -f unix \"contracts/**/*.sol\" --max-warnings 0"
  },
  "files": [
    "src",
    "dist",
    "build/contracts",
    "contracts/*.sol"
  ]
}
